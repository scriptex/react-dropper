{
  "name": "react-dropper",
  "version": "1.1.0",
  "description": "Pick a color from any image in React",
  "keywords": [
    "React Widget",
    "React Canvas",
    "Extract Color",
    "React Component",
    "React Extract Color"
  ],
  "homepage": "https://react-dropper.atanas.info",
  "bugs": {
    "url": "https://github.com/scriptex/react-dropper/issues",
    "email": "hi@atanas.info"
  },
  "license": "MIT",
  "author": "Atanas Atanasov <hi@atanas.info> (https://atanas.info)",
  "funding": "https://github.com/sponsors/scriptex",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "github:scriptex/react-dropper"
  },
  "scripts": {
    "clear": "rm -rf dist && rm -rf demo/dist",
    "start": "tsc -w",
    "build": "yarn clear && tsc",
    "test": "jest --env jsdom",
    "lint": "eslint",
    "deduplicate": "yarn-deduplicate",
    "prod": "yarn deduplicate && yarn lint && yarn build && yarn test"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint": "9.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.2.0",
    "jest": "30.0.3",
    "jest-canvas-mock": "2.5.2",
    "jest-environment-jsdom": "30.0.2",
    "process": "0.11.10",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3",
    "yarn-deduplicate": "6.0.2"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17 || ^18 || ^19",
    "react-dom": "^16.9.0 || ^17 || ^18 || ^19"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
