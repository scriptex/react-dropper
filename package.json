{
  "name": "react-dropper",
  "version": "1.1.0",
  "description": "Pick a color from any image in React",
  "keywords": [
    "React Widget",
    "React Canvas",
    "Extract Color",
    "React Component",
    "React Extract Color"
  ],
  "homepage": "https://react-dropper.atanas.info",
  "bugs": {
    "url": "https://github.com/scriptex/react-dropper/issues",
    "email": "hi@atanas.info"
  },
  "license": "MIT",
  "author": "Atanas Atanasov <hi@atanas.info> (https://atanas.info)",
  "funding": "https://github.com/sponsors/scriptex",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "github:scriptex/react-dropper"
  },
  "scripts": {
    "clear": "rm -rf dist && rm -rf demo/dist",
    "start": "tsc -w",
    "build": "yarn clear && tsc",
    "test": "jest --env jsdom",
    "lint": "eslint",
    "deduplicate": "yarn-deduplicate",
    "prod": "yarn deduplicate && yarn lint && yarn build && yarn test"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@types/jest": "29.5.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "jest": "29.7.0",
    "jest-canvas-mock": "2.5.2",
    "jest-environment-jsdom": "29.7.0",
    "process": "0.11.10",
    "ts-jest": "29.1.3",
    "typescript": "5.4.5",
    "yarn-deduplicate": "6.0.2"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
