{
  "name": "react-dropper",
  "version": "1.1.0",
  "description": "Pick a color from any image in React",
  "keywords": [
    "React Widget",
    "React Canvas",
    "Extract Color",
    "React Component",
    "React Extract Color"
  ],
  "homepage": "https://react-dropper.atanas.info",
  "bugs": {
    "url": "https://github.com/scriptex/react-dropper/issues",
    "email": "hi@atanas.info"
  },
  "license": "MIT",
  "author": "Atanas Atanasov <hi@atanas.info> (https://atanas.info)",
  "funding": "https://github.com/sponsors/scriptex",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "github:scriptex/react-dropper"
  },
  "scripts": {
    "clear": "rm -rf dist && rm -rf demo/dist",
    "start": "tsc -w",
    "build": "yarn clear && tsc",
    "test": "jest --env jsdom",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'demo/**/*.{ts,tsx}' '__tests__/**/*.{ts,tsx}'",
    "prod": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.2",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.2.2",
    "jest-canvas-mock": "2.4.0",
    "jest-environment-jsdom": "29.2.2",
    "process": "0.11.10",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
