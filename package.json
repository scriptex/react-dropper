{
  "name": "react-dropper",
  "version": "0.1.0",
  "description": "Pick color from image in React",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clear": "rm -rf dist",
    "start": "tsc -w",
    "build": "yarn clear && tsc --build tsconfig.json",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prepare": "yarn lint && yarn build && yarn test",
    "demo": "webpack --mode production"
  },
  "keywords": [
    "React",
    "Accordion",
    "Typescript",
    "Widget"
  ],
  "author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
  "repository": "https://github.com/scriptex/react-dropper",
  "bugs": {
    "url": "https://github.com/scriptex/react-dropper/issues",
    "email": "scriptex.bg@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "peerDependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.1.3",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "@types/react-test-renderer": "16.9.2",
    "awesome-typescript-loader": "5.2.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "file-loader": "5.1.0",
    "jest": "25.1.0",
    "jest-css-modules": "2.1.0",
    "react-test-renderer": "16.12.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "25.2.1",
    "tslint": "5.20.1",
    "tslint-react": "4.1.0",
    "typescript": "3.7.5",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/demo/",
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/test-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg|jpg|png)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
